{
  "version": "0.2",
  "language": "en",
  "enableFiletypes": ["markdown"],
  "ignorePaths": [
    "node_modules/**",
    ".git/**",
    "*.log",
    "*.json",
    "*.js",
    "*.ts",
    "*.tsx",
    "*.jsx",
    "*.py",
    "*.sh",
    "*.yml",
    "*.yaml",
    "*.toml",
    "package-lock.json"
  ],
  "files": ["**/*.md"],
  "words": [
    "adrs",
    "avocado",
    "ffmpeg",
    "mfu",
    "semver",
    "tenancy",
    "webhook",
    "whisper"
  ]
}
